{
  "name": "hawkbit/datastream",
  "type": "library",
  "description": "Compressed binary data designed for inter-process communication",
  "keywords": [
    "library",
    "microservices",
    "communication",
    "Inter-process communication"
  ],
  "homepage": "https://github.com/Hawkbit",
  "license": "MIT",
  "authors": [
    {
      "name": "Marco Bunge",
      "email": "marco_bunge@web.de"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Hawkbit\\DataStream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hawkbit\\DataStream\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "cd vendor/phpunit/phpunit",
      "phpunit --configuration phpunit.xml"
    ]
  }
}
